package day4;

public class code5_acess_modifier {
	int varDefault=10;
	public int varPublic=20;
	private int varPrivate=30;
	protected int varProtected=40;
	void methodDefault()
	{
		System.out.println("Default aceess Protected class");
		System.out.println("Default variable: " +varDefault);
	
	}
	public void methodPublic()

	{
		System.out.println("Public aceess base class");
		System.out.println("Public variable: " +varPublic);
	}
	public void methodPrivate()

	{
		System.out.println("Private aceess base class");
		System.out.println("Private variable: " +varPrivate);
	}
	public void methodProtected()

	{
		System.out.println("Protected aceess base class");
		System.out.println("Protected variable: " +varProtected);
	}
	
}
